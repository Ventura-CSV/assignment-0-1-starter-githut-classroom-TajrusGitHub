def main():
    # Prompt the user to enter a number
    number = int(input('Enter your input: 7'))

    # Define the range (for example, 1 to 10)
    min_range = 1
    max_range = 10

    # Check if the number is within the specified range
    if min_range <= number <= max_range:
        result = True  # Number is within the range
        print(f'The value {number} is within the range [{min_range}, {max_range}]')
    else:
        result = False  # Number is outside the range
        print(f'The value {number} is outside the range [{min_range}, {max_range}]')

    return result


if __name__ == '__main__':
    main()
