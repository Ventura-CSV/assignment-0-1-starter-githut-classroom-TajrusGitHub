def calculate_grade(score):
    if 90 <= score <= 100:
        return 'A'
    elif 80 <= score < 90:
        return 'B'
    elif 70 <= score < 80:
        return 'C'
    elif 60 <= score < 70:
        return 'D'
    else:
        return 'F'

def main():
    score = int(input('Enter your score: 520'))

    # Calculate the grade
    grade = calculate_grade(score)

    print(f'Score: {score} \t Grade: {grade}')

    return grade

if __name__ == "__main__":
    main()
