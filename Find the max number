def main():
    num1 = int(input('Enter the first number: 10'))
    num2 = int(input('Enter the second number: 50'))
    num3 = int(input('Enter the third number: 100'))

    # Find the maximum among the three numbers
    maxnum = max(num1, num2, num3)

    print(f'The greatest number is {maxnum}')
    return maxnum

if __name__ == '__main__':
    main()

